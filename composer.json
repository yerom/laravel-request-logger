{
  "name": "jmarcher/laravel-request-logger",
  "description": "HTTP request logger middleware for Laravel",
  "keywords": ["laravel", "logger", "request", "response"],
  "license": "MIT",
  "authors": [
    {
      "name": "Anderson Andrade and Joaquin Marcher",
      "email": "contato@andersonandra.de"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "illuminate/support": "^6.0",
    "illuminate/http": "^6.0",
    "monolog/monolog": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\RequestLogger\\": "src/Prettus/RequestLogger/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
